            android:label="Email認證"/>
        <activity android:name=".AddMenuActivity"
            android:label="新增菜單">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>